{
  "name": "react-native-skia-dev-tools",
  "version": "1.0.0",
  "description": "## Getting started",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^16.11.7",
    "clang-format": "^1.6.0",
    "react": "^16.9.0",
    "react-native": "^0.61.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "build-skia-ios-arm": "ts-node ./scripts/build-externals.ts ios arm",
    "build-skia-ios-arm64": "ts-node ./scripts/build-externals.ts ios arm64",
    "build-skia-ios-x64": "ts-node ./scripts/build-externals.ts ios x64",
    "build-skia-android-arm": "ts-node ./scripts/build-externals.ts android arm",
    "build-skia-android-arm64": "ts-node ./scripts/build-externals.ts android arm64",
    "build-skia-android-x86": "ts-node ./scripts/build-externals.ts android x86",
    "build-skia-android-x64": "ts-node ./scripts/build-externals.ts android x64",
    "build-skia-ios-fatlibs": "ts-node ./scripts/build-ios-fat-lib.ts",
    "build-skia-ios": "yarn build-skia-ios-x64 && yarn build-skia-ios-arm64 && yarn build-skia-ios-arm && yarn build-skia-ios-fatlibs",
    "build-skia-android": "yarn build-skia-android-x64 && yarn build-skia-android-arm64 && yarn build-skia-android-arm",
    "build-skia": "yarn build-skia-ios && yarn build-skia-android",
    "precommit": "yarn lint-java && yarn clang-format",
    "lint-java": "node ./scripts/lint-java.js",
    "clang-format": "yarn clang-format-ios && yarn clang-format-android && yarn clang-format-common",
    "clang-format-ios": "find package/ios/ -iname *.h -o -iname *.m -o -iname *.cpp | xargs clang-format -i",
    "clang-format-android": "find package/android/src/ -iname *.h -o -iname *.m -o -iname *.cpp | xargs clang-format -i",
    "clang-format-common": "find package/cpp/ -iname *.h -o -iname *.m -o -iname *.cpp | xargs clang-format -i"
  },
  "author": "",
  "license": "ISC"
}